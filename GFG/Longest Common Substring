class Solution:
    def longestCommonSubstr(self, X, Y, n, m):
        ans=0
        L = [[None]*(m+1) for i in range(n+1)] 
        for i in range(n+1): 
            for j in range(m+1):
                if i==0 or j==0:
                    L[i][j]=0
                elif X[i-1]==Y[j-1]:
                    L[i][j]=L[i-1][j-1]+1
                    ans=max(ans,L[i][j])
                else:
                    L[i][j]=0
        return ans
