class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        if len(prices)==0:
            return 0
        globalMax = 0
        globalMin = prices[0]
        for i in range(1,len(prices)):
            globalMin=min(globalMin,prices[i])
            globalMax=max(globalMax,prices[i]-globalMin)
        return globalMax
